/*
 *基于数组实现队列
 *
 */

public class ArrayQueue {
    private int[] items;
    private int n=0;
    //队头、队尾指针
    private int head=0;
    private int tail=0;
    
	public ArrayQueue(int capacity){
		this.items=new int[capacity];
		this.n=capacity;
	}
	//入队
	public boolean enqueue(int item){
	     //队满，无法入队
		   if(tail==n)  return false;
           
	     items[tail]=item;
	     ++tail;
	     return true;
	}
	//入队 + 数据迁移
	public boolean enqueue_1(int item){
		//队满
		if(tail==n){
			//队列无空间
			if(head==0) return false;
			//数据迁移
		  for(int i=0;i<head;++i){
		    items[i]=items[head+i];
		  }
		 //更新head&tail
	   tail-=head;
     head=0;
		}
		items[tail]=item;
	  ++tail;
	  return true;
	}
	
	public int dequeue(){
		//队空，无法出队
		if(head==tail)
			return -1;
		
		int temp=items[head];
		++head;
		return temp;
	}
}
